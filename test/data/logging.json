{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "loggers": {
    "my_module": {
      "level": "ERROR",
      "propagate": false
    }
  },
  "root": {
    "level": "ERROR"
  }
}
